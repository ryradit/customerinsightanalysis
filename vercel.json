{
  "version": 2,
  "name": "akasha-consumer-insights",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "GOOGLE_API_KEY": "${GOOGLE_API_KEY}"
  },
  "functions": {
    "src/app/api/analyze/route.ts": {
      "runtime": "nodejs18.x",
      "maxDuration": 30,
      "memory": 1024
    },
    "src/app/api/preview/route.ts": {
      "runtime": "nodejs18.x",
      "maxDuration": 15,
      "memory": 512
    }
  },
  "regions": ["sin1"],
  "framework": "nextjs"
}